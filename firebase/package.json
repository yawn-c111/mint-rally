{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "yarn run build && ./node_modules/.bin/firebase use --clear && ./node_modules/.bin/firebase emulators:start",
    "shell": "yarn run build && ./node_modules/.bin/firebase functions:shell",
    "start": "yarn run shell",
    "deploy:prd": "./node_modules/.bin/firebase use prd && ./node_modules/.bin/firebase deploy --only functions",
    "deploy:stg": "./node_modules/.bin/firebase use mintrallystg-e1bc4 && ./node_modules/.bin/firebase deploy --only functions",
    "logs": "./node_modules/.bin/firebase functions:log",
    "fix": "mkdir dist/node_modules/.bin && cp -r node_modules/.bin/firebase-functions dist/node_modules/.bin && cp -rfu node_modules/firebase-functions/ dist/node_modules"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "@metamask/eth-sig-util": "^5.0.2",
    "@types/seedrandom": "^3.0.4",
    "ethers": "^5.7.2",
    "firebase": "^9.15.0",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^4.1.1",
    "node-zip": "^1.1.1",
    "seedrandom": "^3.0.5",
    "uuid": "^9.0.0",
    "zokrates-js": "^1.1.4"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "firebase-tools": "^11.20.0",
    "typescript": "^4.5.4"
  },
  "private": true
}
